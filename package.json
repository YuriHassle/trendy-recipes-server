{
  "name": "trendy-recipes",
  "version": "1.0.0",
  "description": "REST API for Trendy Recipes",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/src/server.js",
    "dev": "npm run services:up && nodemon",
    "services:up": "docker-compose up -d",
    "services:stop": "docker-compose stop",
    "services:down": "docker-compose -f down",
    "db:migrate": "knex migrate:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuriHassle/trendy-recipes-server.git"
  },
  "keywords": [
    "portfolio",
    "api",
    "node",
    "rest",
    "fastify"
  ],
  "author": "yurihassle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YuriHassle/trendy-recipes-server/issues"
  },
  "homepage": "https://github.com/YuriHassle/trendy-recipes-server#readme",
  "dependencies": {
    "@fastify/type-provider-typebox": "^3.5.0",
    "@sinclair/typebox": "^0.31.17",
    "ajv-errors": "^3.0.0",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^11.0.6",
    "fastify": "^4.23.2",
    "knex": "^2.5.1",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
